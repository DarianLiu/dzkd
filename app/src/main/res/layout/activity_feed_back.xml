<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/view_statue"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_top"
        android:background="@color/colorPrimary" />

    <include layout="@layout/include_toolbar" />

    <EditText
        android:id="@+id/et_back_text"
        style="@style/mine_feed_back_edit_txt"
        android:layout_marginTop="@dimen/p10"/>

    <EditText
        android:id="@+id/et_back_phone"
        style="@style/mine_feed_back_phone_txt"
        android:layout_marginTop="@dimen/p10"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone">

        <ImageView
            android:id="@+id/iv_feed_back_one"
            style="@style/mine_feed_back_img"
             />

        <ImageView
            android:id="@+id/iv_feed_back_two"
            style="@style/mine_feed_back_img" />

        <ImageView
            android:id="@+id/iv_feed_back_three"
            style="@style/mine_feed_back_img"/>
    </LinearLayout>

    <Button
        android:id="@+id/b_feedback"
        android:text="提交"
        style="@style/mine_feed_back_btn"
        android:layout_marginTop="@dimen/p20"
       />

</LinearLayout>